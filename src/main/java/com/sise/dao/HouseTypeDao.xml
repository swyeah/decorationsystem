<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sise.dao.HouseTypeDao">


    <select id="searchAllHouseType" resultType="houseType">
        SELECT * FROM house_type
    </select>
    <select id="searchHouseTypeById" resultType="houseType">
        SELECT * FROM house_type WHERE house_id = #{house_id}
    </select>


</mapper>